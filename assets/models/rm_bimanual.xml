<mujoco>
    <include file="rm_scene.xml"/>
    <include file="rm_dependencies.xml"/>

    <worldbody>
        <include file="rm_liftlink.xml"/>

        <body name="box" pos="-0.5 0 0.3">
            <joint name="red_box_joint" type="free" frictionloss="0.01" />
            <inertial pos="0 0 0" mass="0.05" diaginertia="0.002 0.002 0.002" />
            <geom condim="4" solimp="2 1 0.01" solref="0.01 1" friction="1 0.005 0.0001" pos="0 0 0" size="0.05 0.05 0.3" type="box" name="red_box" rgba="1 0 0 0.5" />
        </body>
    </worldbody>

    <actuator>
    <position name="pleft_joint_1"   ctrllimited="true" ctrlrange="-3.14 3.14" joint="left_joint_1"  kp="80"/>
    <position name="pleft_joint_2"   ctrllimited="true" ctrlrange="-3.14 3.14" joint="left_joint_2"  kp="80"/>
    <position name="pleft_joint_3"   ctrllimited="true" ctrlrange="-3.14 3.14" joint="left_joint_3"  kp="80"/>
