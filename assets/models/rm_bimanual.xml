    <position name="pright_joint_5"  ctrllimited="true" ctrlrange="-3.14 3.14" joint="right_joint_5" kp="65"/>
    <position name="pright_joint_6"  ctrllimited="true" ctrlrange="-3.14 3.14" joint="right_joint_6" kp="65"/>
    <position name="pright_joint_gripper_up"   ctrllimited="true" ctrlrange="-0.1 0" joint="gripper_right_up_joint"  kp="65"/>
    <position name="pright_joint_gripper_down"   ctrllimited="true" ctrlrange="0 0.1" joint="gripper_right_down_joint"  kp="65"/>

    <position name="plift_joint" joint="lift_joint" kp="50" ctrllimited="true" ctrlrange="0 4"/>

    <velocity name="vleft_joint_1"  ctrllimited="true" ctrlrange="-3.14159 3.14159" joint="left_joint_1"  kv="50"/>
    <velocity name="vleft_joint_2"  ctrllimited="true" ctrlrange="-3.14159 3.14159" joint="left_joint_2"  kv="50"/>
    <velocity name="vleft_joint_3"  ctrllimited="true" ctrlrange="-3.14159 3.14159" joint="left_joint_3"  kv="50"/>
    <velocity name="vleft_joint_4"  ctrllimited="true" ctrlrange="-3.14159 3.14159" joint="left_joint_4"  kv="50"/>
    <velocity name="vleft_joint_5"  ctrllimited="true" ctrlrange="-3.14159 3.14159" joint="left_joint_5"  kv="50"/>
