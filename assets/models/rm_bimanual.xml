    <position name="pleft_joint_5"   ctrllimited="true" ctrlrange="-3.14 3.14" joint="left_joint_5"  kp="65"/>
    <position name="pleft_joint_6"   ctrllimited="true" ctrlrange="-3.14 3.14" joint="left_joint_6"  kp="65"/>
    <position name="pleft_joint_gripper_up"   ctrllimited="true" ctrlrange="-0.1 0" joint="gripper_left_up_joint"  kp="65"/>
    <position name="pleft_joint_gripper_down"   ctrllimited="true" ctrlrange="0 0.1" joint="gripper_left_down_joint"  kp="65"/>

    <position name="pright_joint_1"  ctrllimited="true" ctrlrange="-3.14 3.14" joint="right_joint_1" kp="80"/>
    <position name="pright_joint_2"  ctrllimited="true" ctrlrange="-3.14 3.14" joint="right_joint_2" kp="80"/>
    <position name="pright_joint_3"  ctrllimited="true" ctrlrange="-3.14 3.14" joint="right_joint_3" kp="80"/>
